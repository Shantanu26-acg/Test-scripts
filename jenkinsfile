pipeline {
    agent any

    stages {
        stage('Clone Repo') {
            steps {
                git branch: 'main', url: 'https://github.com/Shantanu26-acg/Test-scripts.git'
            }
        }

        stage('Check Python Version') {
            steps {
                bat 'python --version'
            }
        }

        stage('Run One Test File') {
            steps {
                bat 'python POCreation.py'
            }
        }
    }
}

